<%- include('../partials/header') %>

<h1 style="color: maroon" class="center"><u>Movie List</u></h1>

<ul class="movie__index">
  <% for (let movie of movies) { %>
  <li class="movie__listing">
    <img src="<%= movie.imageURL %>" />
    <a href="/movie/<%= movie.id %>"
      class="movie__listing_text">Movie details...</a>
  </li>
  <% } %>
</ul>

<%- include('../partials/footer') %>
